    "@codemirror/next": "^0.16.0",
    "parcel-plugin-inline-source": "^1.0.0",
    "shader-park-core": "^0.2.6",
    "three": "^0.155.0",
    "tweakpane": "^3.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-static-files-copy": "^2.6.0"
  }
}
